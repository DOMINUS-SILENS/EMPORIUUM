# Configuration pgAdmin
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin

# URL de l'API (pour les frontends)
VITE_API_URL=http://localhost:8000/api/v1
